set(PACKAGE_VERSION 0.4.2)

if("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL "0")
  if("${PACKAGE_FIND_VERSION_MINOR}" EQUAL "4")
    if("${PACKAGE_FIND_VERSION_PATCH}" EQUAL "2")
      set(PACKAGE_VERSION_EXACT TRUE)
    elseif("${PACKAGE_FIND_VERSION_PATCH}" LESS "2")
      set(PACKAGE_VERSION_COMPATIBLE TRUE)
    else()
      set(PACKAGE_VERSION_UNSUITABLE TRUE)
    endif()
  else()
    set(PACKAGE_VERSION_UNSUITABLE TRUE)
  endif()
else()
  set(PACKAGE_VERSION_UNSUITABLE TRUE)
endif()
